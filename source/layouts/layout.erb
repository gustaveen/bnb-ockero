<!doctype html>
<html lang="sv-se">
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Bed and Bike Öckerö" %></title>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700,300' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag "all" %>
  </head>

  <body class="<%= page_classes %>">
    <%= partial "partial/header" %>
    <main class="main">
        <%= partial "partial/hero" %>
        <%= partial "partial/intro" %>
        <%= partial "partial/accommodation" %>
        <%= partial "partial/facilities" %>
        <%= partial "partial/contact" %>
    </main>
    <script type="text/javascript"
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC2YkVBMFLNAmeJpioXKBJ5bT40o3dYDwU&libraries=geometry,places,visualization">
    </script>
    <%= javascript_include_tag "all" %>
  </body>
</html>
